		ifFalse: [
			"Tiene nodos padre entonces seguimos subiendo en el arbol"
			|hasDuplicates|
			hasDuplicates := false.
			parents do: [ :aParentPortfolio | hasDuplicates := hasDuplicates or: (aParentPortfolio findRootsAndCheckDuplicatesWith: aPortfolio) ] .
			^hasDuplicates
		]
