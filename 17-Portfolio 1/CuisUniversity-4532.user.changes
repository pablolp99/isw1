attack: aVictim with: aWeapon

	weapons isNil ifTrue: [ weapons := OrderedCollection new ].

	(weapons includes: aWeapon) 
		ifTrue: [ aWeapon attack: aVictim by: self ]
		ifFalse: [ self signalCanNotAttackWithWeaponNotBelongingToRobot ]! !
